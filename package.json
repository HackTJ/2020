{
  "name": "hacktj-2020",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://hacktj.org",
  "dependencies": {
    "@reach/router": "^1.2.1",
    "bootstrap": "^4.4.1",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.12.0",
    "react-map-gl": "^5.1.3",
    "react-scripts": "3.0.1",
    "react-smooth-collapse": "^2.1.0",
    "react-tooltip": "^3.11.1",
    "react-virtualized-auto-sizer": "^1.0.2",
    "typescript": "^3.7.2"
  },
  "scripts": {
    "start": "npm run switch-homepage && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "switch-event": "./switch event",
    "switch-homepage": "./switch homepage",
    "predeploy": "npm run build",
    "deploy-event": "npm run switch-event && npm run build && gh-pages -d build",
    "deploy-homepage": "npm run switch-homepage && npm run build && gh-pages -b master -d build",
    "deploy-all": "npm run deploy-event && npm run deploy-homepage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^2.1.1"
  }
}
