{
  "name": "hacktj-2020",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://hacktj.org/2020",
  "dependencies": {
    "@reach/router": "^1.2.1",
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.0",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.12",
    "react-dom": "^16.8.6",
    "react-map-gl": "^5.0.11",
    "react-scripts": "3.0.1",
    "react-smooth-collapse": "^2.1.0",
    "typescript": "^3.6.3"
  },
  "scripts": {
    "start": "npm run switch-homepage && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "switch-event": "./switch event",
    "switch-homepage": "./switch homepage",
    "predeploy": "npm run build",
    "deploy-event": "npm run switch-event && gh-pages -d build",
    "deploy-homepage": "npm run switch-homepage && gh-pages -b master -d build",
    "deploy-all": "npm run deploy-event && npm run deploy-homepage"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^2.1.1"
  }
}
